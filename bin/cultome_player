#!/usr/bin/env jruby

system("clear")

require 'cultome/version'
puts "Starting Cultome Player v#{CultomePlayer::VERSION}" # aqui poner una frase humorisitca aleatoria

require 'cultome_player'
require 'cultome/core'
# iniciamos el reproductor
CultomePlayer.new.start

puts "Bye!" # aqui poner una frase humorisitca aleatoria
